<h1>To be a mecahnic you must fill out the form below</h1>
<%= form_for(@mechanic) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name%>
    <br />
    <%= f.label :email %>
    <%= f.email_field :email%>
    <br />
    <%= f.label :mechanic_name %>
    <%= f.text_field :mechanic_name%>
    <br />
    <%= f.label :password %>
    <%= f.password_field :password%>
    <br />
 
    <%= f.submit "Create Mechanic Profile"%>
<% end %>