

<h1>These are all of your Vehicle(s)</h1>

<%= link_to "Add a vehicle", new_vehicle_path %>


<div>
<table class="table">
 <thead class="white">
    <tr>
     <th scope="col"></th>
     <th scope="col">Name</th>
     <th scope="col">Make</th>
     <th scope="col">Model</th>
     <th scope="col">Year</th>
     <th scope="col">Mileage</th>
     <th scope="col">Wheel size</th>
     <th scope="col">Color</th>
    </tr>
 </thead>
<tbody class="white">

<tr>
<% @vehicles.each do |v| %>
<th scope="col"></th>
<td><%=v.vehicle_name  %></td>
<td><%=v.make.capitalize %></td>
<td><%=link_to "#{v.model.capitalize}", vehicle_path(v) %></td>
<td><%= v.year %></td>
<td><%= v.mileage %></td>
<td><%=v.wheel_size %></td>
<td><%= v.color %></td>

</tr>
<% end %>
</tbody>
</table>
</div>



<h1><%= link_to "User Profile", user_path(current_user.id)%></h1>

<%= link_to "Log out", logout_path, method: "delete" %>