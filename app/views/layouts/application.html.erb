<!DOCTYPE html>
<html>
  <head>
    <title>MatsukoMechanicsApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  
  <h1>Welcome To Matsuko Mechanics Forum</h1>
    
<nav class="navbar">
      <%= link_to 'Log Out', logout_path, method: :delete%>
      <%= link_to 'All vehicles', vehicles_path, class: 'navlink'%>
      <%= link_to 'Create a vehicle', new_vehicle_path, class: 'navlink' %>
      <%= link_to 'All issues', issues_path, class: 'navlink' %>
  </nav>
    <%= yield %>
  </body>
</html>
