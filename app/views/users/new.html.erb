<h1>Sign up here</h1>

<%= form_for(@user) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name%>

    <%= f.label :user_name %>
    <%= f.text_field :user_name%>

    <%= f.label :email %>
    <%= f.email_field :email%>

    <%= f.label :password %>
    <%= f.password_field :password%>
    
    <%= f.submit "Create Profile"%>
<% end %>