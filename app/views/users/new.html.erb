<h1>Sign up here</h1>
<div class="alert alert-success" role="alert">
<%= form_for(@user) do |f| %>

  <%= render partial: 'layouts/errors', locals: {object: @user} %>

    <%= f.label :name %>
    <%= f.text_field :name%>

    <%= f.label :email %>
    <%= f.email_field :email%>

    <%= f.label :user_name %>
    <%= f.text_field :user_name%>

    <%= f.label :password %>
    <%= f.password_field :password%>
    
    <%= f.submit "Create Profile"%>
<% end %>
<div>